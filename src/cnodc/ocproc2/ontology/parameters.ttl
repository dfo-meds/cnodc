@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix cnodc: <http://cnodc-cndoc.dfo-mpo.gc.ca/ocproc2#> .
@prefix p09: <http://vocab.nerc.ac.uk/collection/P09/current/> .
@prefix p07: <http://vocab.nerc.ac.uk/collection/P07/current/> .
@prefix bufr4B22: <https://codes.wmo.int/bufr4/b/22/> .
@prefix bufr4B01: <https://codes.wmo.int/bufr4/b/01/> .
@prefix pcode: <https://www.isdm.gc.ca/isdm-gdsi/diction/code_search-eng.asp?code=> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cnodc:dataType rdf:type rdf:Property ;
  rdfs:domain skos:Concept ;
  rdfs:range rdfs:Resource .

cnodc:acceptsUnit rdf:type rdf:Property ;
  rdfs:domain skos:Concept ;
  rdfs:range skos:Literal .

cnodc:preferredUnit rdf:type rdf:Property ;
  rdfs:subClassOf cnodc:acceptsUnit .

cnodc:parameterGroup rdf:type rdf:Property ;
  rdfs:domain skos:Concept ;
  rdfs:range skos:Literal .

cnodc:parameters rdf:type skos:ConceptScheme .

cnodc:Latitude rdf:type skos:Concept ;
  skos:prefLabel "Latitude"@en ;
  skos:documentation "Decimal degrees north of the equator"@en ;
  cnodc:preferredUnit "degree_north" ;
  cnodc:dataType xsd:decimal ;
  cnodc:parameterGroup "coordinates" ;
  skos:inScheme cnodc:parameters .


cnodc:Longitude rdf:type skos:Concept ;
  skos:prefLabel "Longitude"@en ;
  skos:documentation "Decimal degrees [-180 to 180] east of the prime meridian"@en ;
  cnodc:preferredUnit "degree_east" ;
  cnodc:dataType xsd:decimal ;
  cnodc:parameterGroup "coordinates" ;
  skos:inScheme cnodc:parameters .


cnodc:Depth rdf:type skos:Concept ;
  skos:prefLabel "Depth"@en ;
  skos:documentation "Depth in distance units beneath the ocean surface" ;
  cnodc:preferredUnit "m" ;
  cnodc:dataType xsd:decimal ;
  cnodc:parameterGroup "coordinates" ;
  skos:inScheme cnodc:parameters .


cnodc:Pressure rdf:type skos:Concept ;
  skos:prefLabel "Pressure"@en ;
  skos:documentation "Hydrostatic pressure measured in pressure units" ;
  cnodc:preferredUnit "Pa" ;
  cnodc:dataType xsd:decimal ;
  cnodc:parameterGroup "coordinates" ;
  skos:inScheme cnodc:parameters .


cnodc:Time rdf:type skos:Concept ;
  skos:prefLabel "Time"@en ;
  skos:documentation "Time in ISO 8601 format (e.g. YYYY-MM-DDTHH:MM:SS) with optional timezone (default UTC)" ;
  cnodc:dataType xsd:dateTimeStamp ;
  cnodc:parameterGroup "coordinates" ;
  skos:inScheme cnodc:parameters .


cnodc:CNODCStation rdf:type skos:Concept ;
  skos:prefLabel "Station UUID"@en ;
  skos:documentation "UUID of the originating station" ;
  cnodc:dataType xsd:string ;
  cnodc:parameterGroup "metadata" ;
  skos:inScheme cnodc:parameters .


cnodc:CNODCMission rdf:type skos:Concept ;
  skos:prefLabel "Mission Name"@en ;
  skos:documentation "Name of the originating mission" ;
  cnodc:dataType xsd:string ;
  cnodc:parameterGroup "metadata" ;
  skos:inScheme cnodc:parameters .


cnodc:CNODCSource rdf:type skos:Concept ;
  skos:prefLabel "Source Name"@en ;
  skos:documentation "Identifier for the source of the data" ;
  cnodc:dataType xsd:string ;
  cnodc:parameterGroup "metadata" ;
  skos:inScheme cnodc:parameters .


cnodc:CNODCInstrumentType rdf:type skos:Concept ;
  skos:prefLabel "Instrument Type"@en ;
  skos:documentation "Type of instrument that collected the information" ;
  cnodc:dataType xsd:string ;
  cnodc:parameterGroup "metadata" ;
  skos:inScheme cnodc:parameters .


cnodc:CNODCProgram rdf:type skos:Concept ;
  skos:prefLabel "Program Name"@en ;
  skos:documentation "Name of the program that collected the profile" ;
  cnodc:dataType xsd:string ;
  cnodc:parameterGroup "metadata" ;
  skos:inScheme cnodc:parameters .


cnodc:CNODCStatus rdf:type skos:Concept ;
  skos:prefLabel "CNODC Record Status"@en ;
  skos:documentation "Status of the data record" ;
  cnodc:dataType xsd:string ;
  cnodc:parameterGroup "metadata" ;
  skos:inScheme cnodc:parameters .


cnodc:CNODCLevel rdf:type skos:Concept ;
  skos:prefLabel "CNODC Processing Level"@en ;
  skos:documentation "Processing level of the data record" ;
  cnodc:dataType xsd:string ;
  cnodc:parameterGroup "metadata" ;
  skos:inScheme cnodc:parameters .


cnodc:CNODCEmbargoUntil rdf:type skos:Concept ;
  skos:prefLabel "CNODC Embargo Until Date"@en ;
  skos:documentation "If present, the record may not be released to the public until the given date has passed." ;
  cnodc:dataType xsd:dateTimeStamp ;
  cnodc:parameterGroup "metadata" ;
  skos:inScheme cnodc:parameters .


cnodc:CNODCDuplicateId rdf:type skos:Concept ;
  skos:prefLabel "CNODC Duplicate ID"@en ;
  skos:documentation "If present, the record is a duplicate of another better record and this is that records ID" ;
  cnodc:dataType xsd:string ;
  cnodc:parameterGroup "metadata" ;
  skos:inScheme cnodc:parameters .


cnodc:CNODCDuplicateDate rdf:type skos:Concept ;
  skos:prefLabel "CNODC Duplicate Date"@en ;
  skos:documentation "If present, the record is a duplicate of another better record and this is the date the CNODC received that record"
  cnodc:dataType xsd:date ;
  cnodc:parameterGroup "metadata" ;
  skos:inScheme cnodc:parameters .




