@prefix eov: <http://cnodc-cndoc.dfo-mpo.gc.ca/ocproc2/eov.ttl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

eov:essentialOceanVariables rdf:type skos:ConceptScheme .


eov:oxygen rdf:type skos:Concept ;
  skos:prefLabel "Oxygen"@en ;
  skos:prefLabel "Oxygène"@fr ;
  skos:documentation "The amount of dissolved oxygen in seawater."@en ;
  skos:documentation "Concentration d'oxygène dissous dans l'eau de mer"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:nutrients rdf:type skos:Concept ;
  skos:prefLabel "Nutrients"@en ;
  skos:prefLabel "Nutriments"@fr ;
  skos:documentation "The amount of inorganic nutrients in seawater [NO₃, NO₂, PO₄, NH₄, Si(OH)₄]"@en ;
  skos:documentation "Concentration des nutriments inorganiques dissous dans l'eau de mer [NO₃, NO₂, PO₄, NH₄, Si(OH)₄]"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:inorganicCarbon rdf:type skos:Concept ;
  skos:prefLabel "Inorganic carbon"@en ;
  skos:prefLabel "Carbone inorganique"@fr ;
  skos:documentation "Total amount of inorganic carbon dissolved in sea water [CO₂, HCO₃-, CO₃²-]"@en ;
  skos:documentation "Quantité totale de carbone inorganique dissous dans l'eau de mer [CO₂, HCO₃-, CO₃²-]"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:dissolvedOrganicCarbon rdf:type skos:Concept ;
  skos:prefLabel "Dissolved organic carbon"@en ;
  skos:prefLabel "Carbone organique dissous"@fr ;
  skos:documentation "The amount of dissolved carbon dioxide in seawater"@en ;
  skos:documentation "Concentration de carbone organique dissous dans l'eau de mer"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:seaSurfaceHeight rdf:type skos:Concept ;
  skos:prefLabel "Sea surface height"@en ;
  skos:prefLabel "Niveau de l'eau"@fr ;
  skos:documentation "The altitude of mean sea level relative to the Earth's geode."@en ;
  skos:documentation "Correspond au niveau d'eau mesuré. Les niveaux sont variables en fonction des saisons et des évènements climatique"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:seaIce rdf:type skos:Concept ;
  skos:prefLabel "Sea ice"@en ;
  skos:prefLabel "Glace de mer"@fr ;
  skos:documentation "The concentration of ice cover over a specified area and extent"@en ;
  skos:documentation "La concentration en couverture de glace sur une zone et une étendue spécifiées"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:seaState rdf:type skos:Concept ;
  skos:prefLabel "Sea state"@en ;
  skos:prefLabel "État de la mer"@fr ;
  skos:documentation "Sea State is the characterization of wave and swell, typically in terms of height, wavelength, period, and directional wave energy flux."@en ;
  skos:documentation "L'état de la mer est la caractérisation des vagues et de la houle, généralement en termes de hauteur, de longueur d'onde, de période, et le flux d'énergie des ondes directionnelles"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:seaSurfaceSalinity rdf:type skos:Concept ;
  skos:prefLabel "Sea surface salinity"@en ;
  skos:prefLabel "Salinité de surface"@fr ;
  skos:documentation "The concentration of dissolved salts in surface seawater."@en ;
  skos:documentation "La concentration de sel dissous dans l'eau de mer de surface"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:seaSurfaceTemperature rdf:type skos:Concept ;
  skos:prefLabel "Sea surface temperature"@en ;
  skos:prefLabel "Température de surface"@fr ;
  skos:documentation "The temperature of surface seawater."@en ;
  skos:documentation "La température de l'eau de mer en surface"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:subSurfaceSalinity rdf:type skos:Concept ;
  skos:prefLabel "Subsurface salinity"@en ;
  skos:prefLabel "Salinité sous la surface"@fr ;
  skos:documentation "The concentration of dissolved salts in subsurface seawater."@en ;
  skos:documentation "La concentration de sel dissous dans l'eau de mer sous la surface"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:subSurfaceTemperature rdf:type skos:Concept ;
  skos:prefLabel "Subsurface temperature"@en ;
  skos:prefLabel "Température sous la surface"@fr ;
  skos:documentation "The temperature of subsurface seawater."@en ;
  skos:documentation "La température de l'eau de mer sous la surface"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:surfaceCurrents rdf:type skos:Concept ;
  skos:prefLabel "Surface currents"@en ;
  skos:prefLabel "Courants de surface"@fr ;
  skos:documentation "The velocity of sea water in surface"@en ;
  skos:documentation "Vitesse et débit de l'eau en surface"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:subSurfaceCurrents rdf:type skos:Concept ;
  skos:prefLabel "Subsurface currents"@en ;
  skos:prefLabel "Courants sous-marins"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:other rdf:type skos:Concept ;
  skos:prefLabel "Other"@en ;
  skos:prefLabel "Autre"@fr ;
  skos:documentation "All data that does not correspond to the current EOVs."@en ;
  skos:documentation "Toutes les données qui ne correspondent pas aux EOVs actuels."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:transientTracers rdf:type skos:Concept ;
  skos:prefLabel "Transient tracers"@en ;
  skos:prefLabel "Traceurs transitoires"@fr ;
  skos:documentation "Transient tracers are man-made chemical compounds released to the atmosphere at known quantities that can be used in the ocean to quantify ventilation, transit time distribution and transport time-scales"@en ;
  skos:documentation "Les traceurs transitoires sont des composés chimiques artificiels rejetés dans l'atmosphère en quantités connues qui peuvent être utilisés dans l'océan pour quantifier la ventilation, la distribution des temps de transit et les échelles de temps de transport."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:particulateMatter rdf:type skos:Concept ;
  skos:prefLabel "Particulate matter"@en ;
  skos:prefLabel "Matière particulaire"@fr ;
  skos:documentation "Total amount of particles (organic & inorganic) suspended in sea water"@en ;
  skos:documentation "Quantité totale de particules (organiques et inorganiques) en suspension dans l'eau de mer"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:nitrousOxide rdf:type skos:Concept ;
  skos:prefLabel "Nitrous oxide"@en ;
  skos:prefLabel "Protoxyde d'azote"@fr ;
  skos:documentation "Nitrous oxide (N2O) is an important climate-relevant trace gas in the Earth’s atmosphere. The ocean-including its coastal areassuch as continental shelves, estuaries and upwelling areas is a major source of N2O and contributesabout 30% to the atmospheric N2O budget."@en ;
  skos:documentation "Le protoxyde d'azote ou azote nitreux (N2O) est un important gaz traceur du climat dans l'atmosphère terrestre. L'océan, y compris ses zones côtières telles que les plateaux continentaux, les estuaires et les zones d'upwelling, est une source majeure de N2O. Elle contribue à environ 30 % du bilan atmosphérique en protoxyde d'azote."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:stableCarbonIsotopes rdf:type skos:Concept ;
  skos:prefLabel "Stable carbon isotopes"@en ;
  skos:prefLabel "Isotopes stables du carbone"@fr ;
  skos:documentation "The utility of carbon-13 isotope (δ13C, the carbon-13 to carbon-12 isotope ratio; 13C/12C) as a tracer of the ocean’s carbon cycle."@en ;
  skos:documentation "L’utilité de l’isotope du carbone-13 (δ13C, le rapport des isotopes du carbone-13 au carbone-12; 13C/12C) comme traceur du cycle du carbone de l’océan."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:phytoplanktonBiomassAndDiversity rdf:type skos:Concept ;
  skos:prefLabel "Phytoplankton biomass and diversity"@en ;
  skos:prefLabel "Biomasse et diversité de phytoplancton"@fr ;
  skos:documentation "Characterization of the biological diversity of phytoplankton, their phenology, vertical distribution, and community composition, and their role in ocean biogeochemistry and ecosystem services."@en ;
  skos:documentation "Caractérisation de la diversité biologique du phytoplancton, de sa phénologie, de sa distribution verticale et de sa composition communautaire, ainsi que le rôle dans la biogéochimie des océans et les services écosystémiques."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:zooplanktonBiomassAndDiversity rdf:type skos:Concept ;
  skos:prefLabel "Zooplankton biomass and diversity"@en ;
  skos:prefLabel "Biomasse et diversité de zooplancton"@fr ;
  skos:documentation "Zooplankton diversity refers to the number of zooplankton species, taxonomic composition, or community structure within a region. Zooplankton diversity influences ecosystem health and productivity through trophic links."@en ;
  skos:documentation "La diversité du zooplancton fait référence au nombre d'espèces de zooplancton, à la composition taxonomique ou à la structure communautaire au sein d'une région. La diversité du zooplancton influence la santé et la productivité de l'écosystème par le biais de liens trophiques."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:fishAbundanceAndDistribution rdf:type skos:Concept ;
  skos:prefLabel "Fish abundance and distribution"@en ;
  skos:prefLabel "Abondance et distribution de poissons"@fr ;
  skos:documentation "Fish abundance describes the biomass or numbers of fish in the ocean. It can be reported in terms of species or taxonomic or functional groups (e.g., small pelagic fish, mesopelagic fish, and tunalike species) in an assemblage, population, stock or area, or globally"@en ;
  skos:documentation "L'abondance des poissons décrit la biomasse ou le nombre de poissons dans l'océan. Il peut être rapporté en termes d'espèces ou de groupes taxonomiques ou fonctionnels (par exemple, petits poissons pélagiques, poissons mésopélagiques et espèces tunal) dans un assemblage, une population, un stock ou une zone, ou à l'échelle mondiale"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:marineTurtlesBirdsMammalsAbundanceAndDistribution rdf:type skos:Concept ;
  skos:prefLabel "Marine turtles, birds, mammals abundance and distribution"@en ;
  skos:prefLabel "Abondance et distribution de tortues marines, oiseaux et mammifères"@fr ;
  skos:documentation "Abundance refers to the number of individuals (turtles, birds or marine mammals) within a population while distribution refers to\nthe geographic or spatial extent of habitats used by individuals in the population."@en ;
  skos:documentation "L'abondance se réfère au nombre d'individus (tortues, oiseaux ou mammifères marins) au sein d'une population tandis que la distribution se réfère à\nl'étendue géographique ou spatiale des habitats utilisés par les individus de la population."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:hardCoralCoverAndComposition rdf:type skos:Concept ;
  skos:prefLabel "Hard coral cover and composition"@en ;
  skos:prefLabel "Composition et couverture des coraux durs"@fr ;
  skos:documentation "Live hard coral cover and the areal extent of a reef are the most important indicators of whether a reef is in a coral-dominated state or not; the composition and diversity of coral taxa is an important index of reef health; coral condition (e.g. bleaching, disease) gives fundamental information on the health of a reef; the size class structure (and recruitment) of hard corals gives fundamental information on the resilience, disturbance history and recovery potential of a reef."@en ;
  skos:documentation "La couverture de coraux durs vivants et l'étendue spatiale des récifs sont les indicateurs les plus importants pour savoir si le récif est ou non dominé par les coraux.  La composition et la diversité des taxons coralliens sont un indice important de la santé du récif ; l'état des coraux (par exemple le blanchissement, les maladies) donne des informations fondamentales sur la santé du récif ; la structure des classes de taille (et le recrutement) des coraux durs donne des informations fondamentales sur la résilience, l'historique des perturbations et le potentiel de récupération du récif."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:seagrassCoverAndComposition rdf:type skos:Concept ;
  skos:prefLabel "Seagrass cover and composition"@en ;
  skos:prefLabel "Composition et couverture des herbiers marins"@fr ;
  skos:documentation "Characterization of seagrass beds according to species and their cover linked to a spatial distribution."@en ;
  skos:documentation "Caractérisation des herbiers marins en fonction des espèces et de leur couverture selon une distribution spatiale."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:macroalgalCanopyCoverAndComposition rdf:type skos:Concept ;
  skos:prefLabel "Macroalgal canopy cover and composition"@en ;
  skos:prefLabel "Composition et couverture de la canopée de macroalgues"@fr ;
  skos:documentation "Characterization of macroalgal forest according to species and their cover linked to a spatial distribution."@en ;
  skos:documentation "Caractérisation des macroalgues en fonction des espèces et de leur couvert selon une distribution spatiale."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:invertebrateAbundanceAndDistribution rdf:type skos:Concept ;
  skos:prefLabel "Invertebrate abundance and distribution"@en ;
  skos:prefLabel "Abondance et distribution d'invertébrés"@fr ;
  skos:documentation "The abundance of invertebrate species describes the biomass or number of individuals in an assemblage and according to a spatial distribution."@en ;
  skos:documentation "L'abondance d'espèces invertébrées décrit la biomasse ou le nombre d'individus dans un assemblage et selon une distribution spatiale."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:microbeBiomassAndDiversity rdf:type skos:Concept ;
  skos:prefLabel "Microbe biomass and diversity"@en ;
  skos:prefLabel "Biomasse et diversité microbienne"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:oceanColour rdf:type skos:Concept ;
  skos:prefLabel "Ocean colour"@en ;
  skos:prefLabel "Couleur des océans"@fr ;
  skos:documentation "The term 'ocean colour' refers to the result of absorption and scattering of spectral radiation emanating from the sun. Light interacts with water and suspended or dissolved matter and encompasses a multitude of biological, biogeochemical and ecological factors that form the colour of water."@en ;
  skos:documentation "Le terme «couleur de l'océan» se réfère au résultat de l'absorption et de la diffusion du rayonnement spectral émanant du soleil. La lumière interagit avec l'eau et les matières en suspension ou dissoutes et englobe une multitude de facteurs biologiques, biogéochimiques et écologiques qui forment la couleur de l'eau."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:oceanSound rdf:type skos:Concept ;
  skos:prefLabel "Ocean sound"@en ;
  skos:prefLabel "Paysage acoustique des océans"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:marineDebris rdf:type skos:Concept ;
  skos:prefLabel "Marine debris"@en ;
  skos:prefLabel "Débris marins"@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:oceanSurfaceHeatFlux rdf:type skos:Concept ;
  skos:prefLabel "Ocean surface heat flux"@en ;
  skos:prefLabel "Flux de chaleur océanique de surface"@fr ;
  skos:documentation "Surface heat flux is the rate of heat exchange, per unit area, passing through the sea surface to the atmosphere. The net heat flux is the sum of the turbulent fluxes (latent and sensitive) and the radiative flux (short and long waves)."@en ;
  skos:documentation "Le flux de chaleur de surface est le taux d'échange de chaleur, par unité de surface, traversant la surface de la mer jusqu'à l'atmosphère. Le flux de chaleur net est la somme des flux turbulents (latents et sensibles) et du flux radiatif (ondes courtes et longues)."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:oceanSurfaceStress rdf:type skos:Concept ;
  skos:prefLabel "Ocean surface stress"@en ;
  skos:prefLabel "Contraintes sur la surface océanique"@fr ;
  skos:documentation "Ocean stress is related to lower atmosphere drag and ocean surface dynamics. The magnitude of the stress influences the exchange of energy between the ocean and the atmosphere (evaporation, oxygen, carbon dioxide and other gases)."@en ;
  skos:documentation "Le stress océanique est lié à la trainée de la basse atmosphère et à la dynamique de surface de l'océan. L'ampleur du stress influence l'échange d'énergie entre l'océan et l'atmosphère (évaporation, oxygène, dioxyde de carbon et gaz autres)."@fr ;
  skos:inScheme eov:essentialOceanVariables .

eov:oceanBottomPressure rdf:type skos:Concept ;
  skos:prefLabel "Ocean Bottom Pressure"@en ;
  skos:prefLabel "Pression au fond de l'océan"@fr ;
  skos:documentation "Ocean bottom pressure is the total pressure resulting from the weight or mass of the column of seawater and the overlaying atmosphere."@en ;
  skos:documentation "La pression au fond de l'océan est la pression totale résultant du poids ou de la masse de la colonne d'eau de mer et de l'atmosphère qui la recouvre."@fr ;
  skos:inScheme eov:essentialOceanVariables .
